<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

    <xacro:include filename="inertial_macros.xacro"/>
    <xacro:property name="sum_mass" value="20.0"/>
    <xacro:property name="chassis_mass" value="17.0"/>
    <xacro:property name="track_mass" value="3"/>
    <xacro:property name="wheel_mass" value="${track_mass/3}"/>


    <xacro:property name="min_depth" value="0.001"/>
    <xacro:property name="mu1" value="0.7"/>
    <xacro:property name="mu2" value="0.7"/>
    <xacro:property name="kp" value="1000000"/>
    <!-- <xacro:property name="kd" value="100"/> -->
    <!-- <xacro:property name="kd" value="100"/> -->


<!-- MATERIALS -->


    <material name="white">
        <color rgba="1 1 1 1" />
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

<!-- BASE LINK -->

    <link name="base_link">

    </link>

<!-- BASE_FOOTPRINT LINK -->

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint">
    </link>

<!-- CHASSIS LINK -->

    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="chassis">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 ${pi}"/>
            <geometry> 
                <mesh filename="file://$(find test_bot)/meshes/2-Chassis.stl" />
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0.0555"/>
            <geometry>
                <box size="0.336 0.18 0.111"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="${chassis_mass}" x="0.336" y="0.18" z="0.111">        
            <origin xyz="0 0 0.0555" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <gazebo reference="chassis">
        <material>Gazebo/White</material>
    </gazebo>


<!-- LEFT WHEEL 1 LINK -->

    <joint name="left_wheel1_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel1"/>
        <origin xyz="0.128 0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel1">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="${wheel_mass}" radius="0.06" length="0.05">          
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="left_wheel1">
        <material>Gazebo/Blue</material>
        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>

    </gazebo>

<!-- LEFT WHEEL 2 LINK -->

    <joint name="left_wheel2_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel2"/>
        <origin xyz="0 0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel2">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="${wheel_mass}" length="0.05" radius="0.06">          
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="left_wheel2">
        <material>Gazebo/Blue</material>

        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>
        <!-- <mu2 value="${mu2}"></mu2> -->

    </gazebo>

<!-- LEFT WHEEL 3 LINK -->

    <joint name="left_wheel3_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel3"/>
        <origin xyz="-0.128 0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel3">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="${wheel_mass}" length="0.05" radius="0.06">         
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="left_wheel3">
        <material>Gazebo/Blue</material>

        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>
        <!-- <mu2 value="${mu2}"></mu2> -->

    </gazebo>

<!-- RIGHT WHEEL 1 LINK -->

    <joint name="right_wheel1_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel1"/>
        <origin xyz="0.128 -0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_wheel1">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="${wheel_mass}" length="0.05" radius="0.06">          
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="right_wheel1">
        <material>Gazebo/Blue</material>

        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>
        <!-- <mu2 value="${mu2}"></mu2> -->

    </gazebo>


<!-- RIGHT WHEEL 2 LINK -->

    <joint name="right_wheel2_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel2"/>
        <origin xyz="0 -0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_wheel2">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>

        <xacro:inertial_cylinder mass="${wheel_mass}" length="0.05" radius="0.06">          
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="right_wheel2">
        <material>Gazebo/Blue</material>

        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>
        <!-- <mu2 value="${mu2}"></mu2> -->

    </gazebo>

<!-- RIGHT WHEEL 3 LINK -->

    <joint name="right_wheel3_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel3"/>
        <origin xyz="-0.128 -0.115 0.04" rpy="-${pi/2} 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_wheel3">
        <collision>
            <geometry>
                <cylinder radius="0.06" length="0.05"/>
            </geometry>
        </collision>

        <xacro:inertial_cylinder mass="${wheel_mass}" length="0.05" radius="0.06">         
           <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="right_wheel3">
        <material>Gazebo/Blue</material>

        <min_depth value="${min_depth}"></min_depth>
        <mu1 value="${mu1}"></mu1>
        <mu2 value="${mu2}"></mu2>
        <kp value="${kp}"></kp>
        <!-- <mu2 value="${mu2}"></mu2> -->

    </gazebo>

<!-- Forward Additional Tracks-->

    <joint name="forward_track_joint" type="revolute">
        <parent link="base_link"/>
        <child link="forward_track_link"/>
        <origin xyz="0.128 0 0.04" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-${pi/2}" upper="${pi/2}" velocity="10" effort="10"/>
        <!-- <dynamics damping="10.0"/>  -->
    </joint>


    <link name="forward_track_link">
        <visual>
            <origin xyz="-0.128 0 0.04" rpy="0 ${pi} 0" />
            <geometry>
                <mesh filename="file://$(find test_bot)/meshes/1_left_forward.dae" />
            </geometry>
            <material name="blue"/>
        </visual>        
        <!-- <collision>
            <origin xyz="0.1 0.1425 0" rpy="${pi/2} 0 0" />
            <geometry>
                <box size="0.2 0.01 0.005"/> 
            </geometry>
        </collision> -->
     
        <visual>
            <origin xyz="-0.128 0 0.04" rpy="0 ${pi} 0"/>
            <geometry>
                <mesh filename="file://$(find test_bot)/meshes/1_right_forward.dae" />
            </geometry>
        </visual>        
        <!-- <collision>
            <origin xyz="0.1 -0.1425 0" rpy="${pi/2} 0 0" />
            <geometry>
                <box size="0.2 0.01 0.005"/>
            </geometry>
        </collision> -->

        <xacro:inertial_box mass="3" x="0.15" y="0.1" z="0.05">         
           <origin xyz="0.07 0 0.04" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <gazebo reference="forward_track_link">
        <material>Gazebo/white</material>
        <!-- <mu1>1.5</mu1>  
        <mu2>1.5</mu2>   
        <kp>1e6</kp>      
        <kd>100.0</kd>   
        <fdir1>1 0 0</fdir1>  -->
    </gazebo>
  
<!-- Backward Additional Tracks-->

    <joint name="backward_track_joint" type="revolute">
        <parent link="chassis"/>
        <child link="backward_track_link"/>
        <origin xyz="-0.128 0 0.04" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-${pi/2}" upper="${pi/2}" velocity="100" effort="100"/>
        <!-- <dynamics damping="10.0"/>  -->
    </joint>


    <link name="backward_track_link">
        
        <visual>
            <origin xyz="0.128 0 -0.04" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find test_bot)/meshes/1_left_forward.dae" />
            </geometry>
        </visual>        
        <!-- <collision>
            <origin xyz="-0.1 0.1425 0" rpy="${pi/2} 0 0" />
            <geometry>
                <box size="0.2 0.01 0.005"/>
            </geometry>
        </collision> -->
     
        <visual>
            <origin xyz="0.128 0 -0.04" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find test_bot)/meshes/1_right_forward.dae" />
            </geometry>
        </visual>        
        <!-- <collision>
            <origin xyz="-0.1 -0.1425 0" rpy="${pi/2} 0 0" />
            <geometry>
                <box size="0.2 0.01 0.005"/>
            </geometry>
        </collision> -->



        <xacro:inertial_box mass="3" x="0.15" y="0.1" z="0.05">         
           <origin xyz="-0.07 0 0.04" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <gazebo reference="backward_track_link">
        <material>Gazebo/white</material>
    </gazebo>

</robot>